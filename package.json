{
  "name": "chatlink",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "client": "cd client && yarn && yarn dev",
    "build-client": "cd client && yarn && yarn build",
    "build-server": "docker build -t chatlink/server server/express",
    "server": "docker compose -f server/express/docker-compose.yml up",
    "server-down": "docker compose -f server/express/docker-compose.yml down",
    "mongo": "docker compose -f server/docker-compose.yml up -d",
    "mongo-down": "docker compose -f server/docker-compose.yml down",
    "create-network": "docker network create -d bridge chatlink_network"
  },
  "author": "Ogulcan Pirim",
  "license": "MIT",
  "dependencies": {
    "multer": "^1.4.5-lts.1"
  }
}
